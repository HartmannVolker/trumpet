{
  "name": "trumpet",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "rm -f public/sw.js && next",
    "build": "next build",
    "start": "next start",
    "serve": "yarn build && yarn start",
    "type-check": "tsc",
    "test": "jest",
    "test:cypress": "cypress run --record --parallel",
    "test:integration": "start-server-and-test serve http://localhost:3000 test:cypress"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.18.10",
    "@material-ui/core": "^4.10.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.12.1",
    "next": "^9.4.4",
    "next-absolute-url": "^1.2.2",
    "next-pwa": "^2.6.1",
    "next-translate": "^0.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-recaptcha-v3": "^1.5.2",
    "react-hook-form": "^5.7.2",
    "telegraf": "^3.38.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.1",
    "@types/brainhubeu__react-carousel": "^1.15.0",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^9.8.1",
    "cypress": "^4.8.0",
    "firebase-tools": "^8.4.3",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.0.1",
    "jimp": "^0.13.0",
    "nyc": "^15.1.0",
    "postcss-import": "^12.0.1",
    "start-server-and-test": "^1.11.0",
    "tailwindcss": "^1.4.6",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "vercel": "^19.1.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/integration"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "clearMocks": true,
    "coverageDirectory": "coverage"
  }
}
