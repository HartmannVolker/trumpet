{
  "name": "trumpet",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "rm -f public/sw.js && next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "test": "jest",
    "test:cypress": "cypress run --record --parallel",
    "test:integration": "start-server-and-test start http://localhost:3000 test:cypress"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.12.1",
    "next": "^9.4.2",
    "next-absolute-url": "^1.2.2",
    "next-i18next": "^5.0.0-beta.2",
    "next-pwa": "^2.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-recaptcha-v3": "^1.5.1",
    "react-hook-form": "^5.7.2",
    "telegraf": "^3.38.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "autoprefixer": "^9.8.0",
    "cypress": "^4.6.0",
    "firebase-tools": "^8.4.0",
    "jest": "^26.0.1",
    "jimp": "^0.12.1",
    "postcss-import": "^12.0.1",
    "start-server-and-test": "^1.11.0",
    "tailwindcss": "^1.4.6",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3",
    "vercel": "^19.0.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/test/integration"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "clearMocks": true,
    "coverageDirectory": "coverage"
  }
}
