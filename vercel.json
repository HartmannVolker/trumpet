{
  "version": 2,
  "github": {
    "silent": true
  },
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "X-Frame-Options": "sameorigin",
        "Content-Security-Policy": "default-src 'self' 'https://www.google.com/recaptcha/' 'https://www.gstatic.com/recaptcha/'",
        "Feature-Policy": "fullscreen 'self'",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "same-origin"
      },
      "continue": true
    }
  ]
}